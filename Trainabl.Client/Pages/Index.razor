@page "/"
@inject NavigationManager NavigationManager

<AuthorizeView>
    <Authorized>
        @{
            NavigationManager.NavigateTo("TrainerDashboard");
        }
    </Authorized>
    <NotAuthorized>
        @{
            NavigationManager.NavigateTo("authentication/login");
        }
    </NotAuthorized>
</AuthorizeView>